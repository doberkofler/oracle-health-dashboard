{
  "name": "oracle-health-dashboard",
  "version": "0.0.1",
  "description": "NodeJS based dashboard to monitor Oracle instances.",
  "author": "Dieter Oberkofler <doberkofler@users.noreply.github.com",
  "license": "Apache-2.0",
  "exports": "./build/src/index.js",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "start": "nodemon --watch out --watch config out/server/index.js start --config=./config/config.json",
    "ci": "npm run clean && npm run lint && npm run test && npm run build:d",
    "clean": "rimraf coverage build tmp && jest --clearCache",
    "build:d": "npm run clean && ts-node scripts/build.ts --development",
    "build:p": "npm run clean && ts-node scripts/build.ts --production",
    "watch:d": "nodemon --ext js,jsx,ts,tsx,css --watch src --watch scripts --watch node_modules --exec \"ts-node scripts/build.ts --development\"",
    "lint": "eslint . --ext .ts,.tsx && tsc --noEmit",
    "test": "jest --coverage",
    "test:watch": "jest --watch --coverage=false"
  },
  "dependencies": {
    "@emotion/react": "11.10.6",
    "@emotion/styled": "11.10.6",
    "@mui/icons-material": "5.11.16",
    "@mui/material": "5.12.2",
    "@mui/x-data-grid": "6.2.1",
    "@tanstack/react-query": "4.29.5",
    "axios": "1.3.6",
    "commander": "10.0.1",
    "compression": "1.7.4",
    "date-fns": "2.29.3",
    "debug": "4.3.4",
    "express": "4.18.2",
    "fs-extra": "11.1.1",
    "morgan": "1.10.0",
    "nodemon": "2.0.22",
    "oracledb": "5.5.0",
    "ping": "0.4.4",
    "portfinder": "1.0.32",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.10.0",
    "recharts": "2.5.0",
    "rimraf": "5.0.0",
    "threads": "1.7.0",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@types/compression": "1.7.2",
    "@types/debug": "4.1.7",
    "@types/express": "4.17.17",
    "@types/fs-extra": "11.0.1",
    "@types/glob": "8.1.0",
    "@types/jest": "29.5.1",
    "@types/morgan": "1.9.4",
    "@types/node": "18.16.0",
    "@types/oracledb": "5.2.5",
    "@types/ping": "0.4.1",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.1",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.59.1",
    "@typescript-eslint/parser": "5.59.1",
    "esbuild": "0.17.18",
    "eslint": "8.39.0",
    "eslint-plugin-jest": "27.2.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "jest": "29.5.0",
    "supertest": "6.3.3",
    "ts-jest": "29.1.0",
    "ts-node": "10.9.1",
    "tslib": "2.5.0",
    "tsutils": "3.21.0",
    "typescript": "5.0.4"
  }
}
