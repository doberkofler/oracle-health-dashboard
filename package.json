{
  "name": "oracle-health-dashboard",
  "version": "0.0.1",
  "description": "NodeJS based dashboard to monitor Oracle instances.",
  "exports": "./build/src/index.js",
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "compression": "1.7.4",
    "date-fns": "2.29.3",
    "debug": "4.3.4",
    "express": "4.18.2",
    "oracledb": "5.5.0",
    "ping": "0.4.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "threads": "1.7.0",
    "tslib": "2.4.1",
    "yargs": "17.6.2"
  },
  "devDependencies": {
    "@types/compression": "1.7.2",
    "@types/debug": "^4.1.7",
    "@types/express": "4.17.15",
    "@types/jest": "29.2.4",
    "@types/node": "18.11.16",
    "@types/oracledb": "5.2.3",
    "@types/ping": "0.4.1",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "@types/supertest": "2.0.12",
    "@types/yargs": "17.0.17",
    "@typescript-eslint/eslint-plugin": "5.46.1",
    "@typescript-eslint/parser": "5.46.1",
    "esbuild": "0.16.7",
    "eslint": "8.29.0",
    "eslint-plugin-jest": "27.1.7",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "fs-extra": "11.1.0",
    "jest": "29.3.1",
    "nodemon": "2.0.20",
    "rimraf": "3.0.2",
    "supertest": "6.3.3",
    "ts-jest": "29.0.3",
    "tsutils": "3.21.0",
    "typescript": "4.9.4"
  },
  "scripts": {
    "start": "nodemon --watch out --watch config out/server/index.js --config=./config/config.json",
    "clean": "rimraf coverage build tmp && jest --clearCache",
    "build": "npm run clean && node build.js",
    "build:watch": "nodemon --ext ts,tsx,css --watch src build.js",
    "lint": "eslint . --ext .ts,.tsx && tsc --noEmit",
    "test": "jest --coverage=false",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch --coverage=false"
  },
  "author": "Dieter Oberkofler <doberkofler@users.noreply.github.com",
  "license": "Apache-2.0",
  "volta": {
    "node": "16.13.0"
  }
}
